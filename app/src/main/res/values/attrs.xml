<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>
    <declare-styleable name="CvCameraPreview">
        <attr name="camera_type" format="integer">
            <enum name="back" value="99" />
            <enum name="front" value="98" />
        </attr>
        <attr name="scale_type" format="integer">
            <enum name="fit" value="1" />
            <enum name="full" value="2" />
        </attr>
    </declare-styleable>
</resources>